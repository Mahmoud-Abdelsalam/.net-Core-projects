@model int

@{
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mt-3 mb-2">
    <h4>404 Not Found Error :</h4>
    <hr />
    <h5>
        Employee with Id = @Model cannot be found
    </h5>

</div>
<a asp-controller="Home" asp-action="Index"  class="btn btn-outline-success" >Click here to see the available list of employees</a>